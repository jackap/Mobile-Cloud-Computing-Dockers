<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_benchmark"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.mcc.ocr.BenchmarkActivity">

    <RelativeLayout
        android:id="@+id/rel_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:text="Remote OCR Results"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:id="@+id/textView"
            android:textSize="24sp"
            android:textStyle="normal|bold" />

        <TextView
            android:text="Total execution time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView"
            android:layout_alignParentStart="true"
            android:id="@+id/textView2" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="67dp"
            android:id="@+id/tv1"
            tools:text="0s" />

        <TextView
            android:text="Maximum execution time	"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView2"
            android:layout_alignParentStart="true"
            android:id="@+id/textView11" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv1"
            android:layout_alignEnd="@+id/tv1"
            android:id="@+id/tv2"
            tools:text="0s" />

        <TextView
            android:text="Minimum execution time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView11"
            android:layout_alignParentStart="true"
            android:id="@+id/textView13" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv2"
            android:layout_alignEnd="@+id/tv2"
            android:id="@+id/tv3"
            tools:text="0s" />

        <TextView
            android:text="Average execution time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView13"
            android:layout_alignParentStart="true"
            android:id="@+id/textView15" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv4"
            tools:text="0s"
            android:layout_below="@+id/tv3"
            android:layout_alignStart="@+id/tv3" />

        <TextView
            android:text="Data Exchanged with the sever"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView15"
            android:layout_alignParentStart="true"
            android:id="@+id/textView111" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvNet"
            tools:text="0B"
            android:layout_below="@+id/tv4"
            android:layout_alignStart="@+id/tv4" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rel_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp">
        <TextView
            android:text="Local OCR Results"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:id="@+id/textView_2"
            android:textSize="24sp"
            android:textStyle="normal|bold" />

        <TextView
            android:text="Total execution time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView_2"
            android:layout_alignParentStart="true"
            android:id="@+id/textView2_2" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView_2"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="67dp"
            android:id="@+id/tv5"
            tools:text="0s" />

        <TextView
            android:text="Maximum execution time	"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView2_2"
            android:layout_alignParentStart="true"
            android:id="@+id/textView11_2" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv5"
            android:layout_alignEnd="@+id/tv5"
            android:id="@+id/tv6"
            tools:text="0s" />

        <TextView
            android:text="Minimum execution time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView11_2"
            android:layout_alignParentStart="true"
            android:id="@+id/textView13_2" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv6"
            android:layout_alignEnd="@+id/tv6"
            android:id="@+id/tv7"
            tools:text="0s" />

        <TextView
            android:text="Average execution time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView13_2"
            android:layout_alignParentStart="true"
            android:id="@+id/textView15_2" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv8"
            tools:text="0s"
            android:layout_below="@+id/tv7"
            android:layout_alignStart="@+id/tv7" />
    </RelativeLayout>


</LinearLayout>
